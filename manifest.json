  {
    "manifest_version":2,
    "name":"Email Composer",
    "version":"1.0",
    "description":"composes email",
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "demo.html"
      },
      "content_scripts":[
        {
          "matches": ["<all_urls>"],
          "js": ["conDom.js"],
          "run_at":"document_start"
        }
      ],
    "permissions":[
        "storage",
        "<all_urls>",
        "tabs"
    ]
}